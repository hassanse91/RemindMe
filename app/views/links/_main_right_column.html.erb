<div class="column col-sm-10 col-xs-11" id="main">
                
                <!-- top nav -->
              	
                <div class="col-md-6 col-md-offset-3">
  <h2>Your Links</h2>
      <% @links.each do |link| %>
            <div class="thumbnail">
           <h2><%= link.title %></h2> 
          <em><%= link.content %></em> 
         <%= link_to "show", link, class: "btn btn-primary" %>         
         </div>
      <% end %>
</div>
            </div>