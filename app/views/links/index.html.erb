<% if !user_signed_in? %>
  <%= render 'links/home' %>
<% else %>
<p id="notice"><%= notice %></p>
<div id="links">
<div class="col-md-6 col-md-offset-3">
  <h1>Your Links</h1>

      <% @links.each do |link| %>
          <div class="link">
            <div class="thumbnail">
           <h2><%= link.title %></h2> 
           <p><%= link.content %></p> 
         <%= link_to "show", link, class: "btn btn-primary" %>
            </div>
         </div>
      <% end %>
</div>
 </div>
 <% end %>